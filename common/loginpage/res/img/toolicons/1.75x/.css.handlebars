{{#spritesheet}}
.pixel-ratio__1_75 {
    .tool-icon, .menu__icon {
        background-image: data-uri('{{{escaped_image}}}');
        background-size: {{scaled width 1.75}}px auto;
    }
}
{{/spritesheet}}
{{#sprites}}
.tool-icon.{{name}} {
    background-position-y: {{scaled offset_y 1.75}}px;
}
{{/sprites}}

